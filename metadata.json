{
  "name": "simp-tpm",
  "version": "3.2.0",
  "author": "SIMP Team",
  "summary": "Manages TPM 1.2 and TBOOT",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-tpm",
  "project_page": "https://github.com/simp/pupmod-simp-tpm",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "tpm",
    "pkcs11",
    "txt",
    "intel",
    "tboot"
  ],
  "dependencies": [
    {
      "name": "simp/ima",
      "version_requirement": ">= 0.1.0 < 1.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.5.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 7.0.0"
    },
    {
      "name": "puppet/yum",
      "version_requirement": ">= 2.0.0 < 5.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
      {
        "name": "simp/rsync",
        "version_requirement": ">= 6.0.0 < 7.0.0"
      }
    ]
  },
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 7.0.0"
    }
  ]
}
