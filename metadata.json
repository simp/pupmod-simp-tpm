{
  "name": "simp-tpm",
  "version": "1.0.0",
  "author": "SIMP Team",
  "summary": "manages TPMs and IMA (if your system supports it)",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-tpm",
  "project_page": "https://github.com/simp/pupmod-simp-tpm",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "tpm",
    "ima",
    "pkcs11"
  ],
  "dependencies": [
    {
      "name": "herculesteam/augeasproviders",
      "version_requirement": ">= 2.1.3 < 3.0.0"
    },
    {
