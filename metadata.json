{
  "name": "simp-tpm",
  "version": "1.1.0",
  "author": "SIMP Team",
  "summary": "manages TPMs and IMA (if your system supports it)",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-tpm",
  "project_page": "https://github.com/simp/pupmod-simp-tpm",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "tpm",
    "ima",
    "pkcs11",
    "txt",
    "intel",
    "tboot"
  ],
  "dependencies": [
    {
      "name": "herculesteam/augeasproviders_grub",
      "version_requirement": ">= 2.3.0 < 4.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.1.0 < 4.0.0"
    },
    {
      "name": "simp/rsync",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 5.0.0"
    },
    {
      "name": "puppet/yum",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
